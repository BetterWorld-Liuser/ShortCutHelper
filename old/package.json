{
  "dependencies": {
    "active-win": "^6.3.0",
    "axios": "^0.21.1",
    "fs-extra": "^9.1.0",
    "hotkeys-js": "^3.8.3",
    "lowdb": "^1.0.0",
    "md5": "^2.3.0",
    "sortablejs": "^1.13.0",
    "uuid": "^8.3.2",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "electron": "^12.0.2",
    "electron-rebuild": "^2.3.5"
  },
  "name": "shortcuthelper",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "hottest": "node hotkeytest.js",
    "test": "./node_modules/.bin/electron ."
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "build": {
    "productName": "shortCutHelper",
    "appId": "top.liuzhengdong.shortCut",
    "copyright": "liuser",
    "directories": {
      "output": "build"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./dist/icons/document128good.png",
      "uninstallerIcon": "./dist/icons/document128good.png",
      "installerHeaderIcon": "./dist/icons/document128good.png",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "快捷键助手"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "win": {
      "icon": "./dist/icons/document128good.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  }
}
